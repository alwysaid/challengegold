    crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
    integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
    crossorigin="anonymous"></script>
</head>



<body class="bg-primary-subtle">
    <div class="sticky-top">
        <div class="containerNav">
            <div class="row">
                <div class="col-md-1">

                </div>
                <div class="col-md-10 px-1">
                    <nav class="navbar-md bg-body-tertiary ">
                        <div class="container-fluid d-flex justify-content-center align-items-center flex-column">
                            <form class="d-flex justify-content-center align-items-center mt-3" role="search">
                                <img src="/asset/Logo2.png" alt="" style="width: 50px;">
                                <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                                <button class="btn btn-outline-info" type="submit">Search</button>
                            </form>
                            <a class="btn btn-info" href="http://localhost:3000/api/v1/login-penggalang" role="button"
                                style="padding-left: 100px; padding-right: 100px; margin-bottom: 3px;">Login Penggalang</a>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </div>


    <div class="container2">
        <div class="row" style="padding-right: 9px;">
            <div class="col-md-1">

            </div>

            <div class="col-md-10 bg-info-subtle shadow m-1">
                <form class="row px-1" action="register-penggalang" method="post">
                    <div class="row">
                        <div class="col-md-4 mt-5 pe-2 border rounded-3 shadow bg-success-subtle mb-5">
                            <h2 class="text-black text-wrap">Daftar Penggalang
                                Sekarang</h2>
                        </div>
                    </div>
                    <div class="row mx-0">
                        <div class="row">
                            <div class="col-lg-8 col-md-6 col-sm-12 mt-2 ">
                                <label for="namaOrganisasi" class="form-label">Nama Organisasi</label>
                                <input type="text" class="form-control" id="namaOrganisasi" name="namaOrganisasi">
                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-12 mt-2">
                                <label for="jenisOrganisasi" class="form-label">Jenis Organisasi</label>
                                <select id="jenisOrganisasi" name="jenisOrganisasi" class="form-control">
                                    <option>NGO</option>
                                    <option>Start-Up</option>
                                    <option>UMKM</option>
                                  </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 mt-2">
                                <label for="lokasiOrganisasi" class="form-label">Lokasi Organisasi</label>
                                <input type="text" name="lokasiOrganisasi" class="form-control" id="lokasiOrganisasi">
                            </div>
                            <div class="col-md-12 mt-2">
                                <label for="deskripsiOrganisasi" class="form-label">Deskripsi Organisasi</label>
                                <textarea type="text" name="deskripsiOrganisasi" class="form-control"
                                    id="deskripsiOrganisasi" cols="70" rows="10"
                                    placeholder="Tulis Deskripsi Organisasi Disini"></textarea>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12 mt-2 ">
                                <label for="picOrganisasi" class="form-label">PIC Organisasi</label>
                                <input type="text" class="form-control" id="picOrganisasi" name="picOrganisasi">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4 col-sm-12 mt-2">
                                <label for="noPicOrganisasi" class="form-label">No. PIC Organisasi</label>
                                <input type="text" class="form-control" id="noPicOrganisasi" name="noPicOrganisasi" placeholder="+62.........">
                            </div>
                            <div class="col-md-4 col-sm-6 mt-2">
                                <label for="emailOrganisasi" class="form-label">Email Organisasi</label>
                                <input type="text" name="emailOrganisasi" class="form-control" id="emailOrganisasi"
                                    placeholder="">
                            </div>
                            <div class="col-md-4 col-sm-6 mt-2">
                                <label for="passwordEmailOrganisasi" class="form-label">Password Organisasi</label>
                                <input type="password" name="passwordEmailOrganisasi" class="form-control"
                                    id="passwordEmailOrganisasi" placeholder="">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 mt-2">
                                <label for="logoOrganisasi" class="form-label">Logo Organisasi</label>
                                <input type="text" name="logoOrganisasi" class="form-control"
                                    id="logoOrganisasi" placeholder="Upload Logo Organisasi">
                            </div>
                        </div>
                        <div class="col-12">
                            <button type="submit" class="btn btn-primary mt-5">Buat Akun Organisasi</button>
                        </div>
                    </div>
                </form>

            </div>
            <div class="col-md-1">

            </div>
        </div>
    </div>

    <div class="containerFooter">
        <div class="row" style="padding-left: 3px; padding-right: 4px;">
            <div class="col-md-1">

            </div>
            <div class="col-md-10">
                <div class="row">
                    <div class="container bg-body-tertiary">
                        <footer class="">
                            <ul class="nav justify-content-center">
                                <li class="nav-item"><a href="#" class="nav-link px-2 text-body-secondary"><img
                                            src="/asset/LogoHome.png" style="width: 50px;" alt="" class="img-fluid"></a>
                                </li>
                                <li class="nav-item"><a href="http://localhost:3000/api/v1/create-campaign"
                                        class="nav-link px-2 text-body-secondary"><img src="/asset/LogoGD.png"
                                            style="width: 50px;" alt="" class="img-fluid"></a></li>
                                <li class="nav-item"><a href="http://localhost:3000/api/v1/register-penggalang"
                                        class="nav-link px-2 text-body-secondary"><img src="/asset/LogoAkun.png"
                                            style="width: 50px;" alt="" class="img-fluid"></a></li>
                            </ul>
                            <div class="d-flex justify-content-center">
                                <p class="text-center text-body-secondary mx-1"
                                    style="padding-right: 10px; padding-left: 33px;">Home</p>
                                <p class="text-center text-body-secondary" style="padding-right: 5px">Campaign</p>
                                <p class="text-center text-body-secondary" style="padding-right: 35px;">Daftar</p>
                            </div>
                            <p class="text-center text-body-secondary">Â© 2024 Company, Inc</p>
                        </footer>
                    </div>
                </div>
            </div>

        </div>
        <div class="col-md-1">

        </div>
    </div>
    </div>
</body>
</html>